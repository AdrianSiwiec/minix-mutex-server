alias cdd="cd /mnt/shared"
alias cds="cd /usr/src/servers/cs"
alias cdl="cd /usr/src/lib/libc/sys-minix/"
alias pull="/mnt/shared/pullMinix.sh"
alias push="/mnt/shared/pushMinix.sh"
alias makecs="cdd; ./pullMinix.sh; cds; make; make install; echo 'cs down...'; service down cs; echo 'cs up...'; service up /usr/sbin/cs; cd"
alias makelib="pull; cd /usr/src; make do-lib"
alias makeincludes="pull; cd /usr/src; make includes"
alias makepm="pull; cd /usr/src/releasetools; make services; make install"
alias copyetc="pull; cp /usr/src/etc/usr/rc /usr/etc"
